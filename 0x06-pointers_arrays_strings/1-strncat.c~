/* This program concatenate two strings based on the length */
#include "main.h"
/**
* *_strncat - This function join two string
* @dest: pointer to char
* @src: pointer to char
* Return: dest
*/
char *_strncat(char *dest, char *src, int n)
{
	int n, i;
	
	n = i = 0;
	/* find the lenght */

	while (*(dest + n))
		n++;
	/* for loop */
	while (i < n && *(src + i))
	{
		*(dest + cn = *(src + i);
		n++;
		i++;
	}
	if (i < n)
	dest = '\0';
	return (dest);
}

